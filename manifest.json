{
  "manifest_version": 3,
  "name": "去做什么事呢",
  "version": "1.0",
  "description": "简单的专注辅助工具",
  "permissions": ["storage", "alarms","notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "speed.png",
      "48": "speed.png",
      "128": "speed.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["audio.mp3"],
      "matches": ["<all_urls>"]
    }
  ]
}
